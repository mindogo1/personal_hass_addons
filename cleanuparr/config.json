  "startup": "services",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 11011,
  "panel_icon": "mdi:delete-sweep",
  "panel_title": "Cleanuparr",
  "arch": ["amd64", "aarch64"],
  "image": "ghcr.io/cleanuparr/cleanuparr:latest",
  "init": false,
  "map": ["config:rw"],
  "environment": {
    "PORT": "11011",
    "TZ": "Etc/UTC",
    "PUID": "1000",
    "PGID": "1000"
  },
  "ports": {
    "11011/tcp": null
  }
}